class Person {
    constructor (name, age, occcupation) {
        this.name= name;
        this.age = age;
        this.occcupation= occcupation;

    }
}
const person = new Person("Harikrishanan", 25, "Trainer");
console.log ("Name:", person.name);
console.log ("Age:", person.age);
console.log ("occupation:", person.occcupation);
